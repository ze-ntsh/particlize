{
  "name": "particlize",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./samplers": {
      "import": "./samplers/index.js",
      "require": "./samplers/index.cjs",
      "types": "./samplers/index.d.ts"
    },
    "./utils": {
      "import": "./utils/index.js",
      "require": "./utils/index.cjs",
      "types": "./utils/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf build",
    "build:js": "vite build",
    "build:types": "dts-bundle-generator --config dts-bundle-generator.config.ts",
    "build": "npm run clean && npm run build:js && npm run build:types",
    "postbuild": "node scripts/postbuild.js"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@types/three": "^0.176.0",
    "@vitest/coverage-v8": "^2.1.8",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^9.5.1",
    "postcss": "^8.4.49",
    "postcss-scss": "^4.0.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "three": "^0.176.0"
  }
}
